<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dishdiscovery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginDD">
        <activity
            android:name="com.example.dishdiscovery.AllergyActivity"
            android:exported="false" />
        <activity
            android:name="com.example.dishdiscovery.DietActivity"
            android:exported="false" />
        <activity
            android:name="com.example.dishdiscovery.CuisineActivity"
            android:exported="false" />
        <activity
            android:name="com.example.dishdiscovery.UserProfile"
            android:exported="false" />
        <activity
            android:name="com.example.dishdiscovery.PasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.example.dishdiscovery.EmailActivity"
            android:exported="false" />
        <activity
            android:name="com.example.dishdiscovery.UserSignActivity"
            android:exported="false" />
        <activity
            android:name="com.example.dishdiscovery.HomeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.dishdiscovery.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>