<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.ui.allergy.AllergyFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="-56dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <View
                android:id="@+id/ellipse_1"
                android:layout_width="661dp"
                android:layout_height="350dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="-138dp"
                android:layout_marginTop="-119dp"
                android:background="@drawable/ellipse_1">

            </View>

            <View
                android:id="@+id/rectangle_3"
                android:layout_width="match_parent"
                android:layout_height="400dp"
                android:layout_marginTop="155dp"
                android:background="@drawable/rect_round" />
        </FrameLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <View
                android:id="@+id/fishCircle"
                android:layout_width="54.25dp"
                android:layout_height="49.23dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="545dp"
                android:background="@drawable/ellipse_allergy"
                />
            <View
                android:id="@+id/shellFishCircle"
                android:layout_width="54.25dp"
                android:layout_height="49.23dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="377dp"
                android:background="@drawable/ellipse_allergy"
                />

            <View
                android:id="@+id/soyCircle"
                android:layout_width="54.25dp"
                android:layout_height="49.23dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="490dp"
                android:background="@drawable/ellipse_allergy"
                />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/peanuts"
                android:layout_width="54.25dp"
                android:layout_height="49.23dp"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="168dp"
                android:src="@drawable/peanuts">
            </com.google.android.material.imageview.ShapeableImageView>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/treeNut"
                android:layout_width="54.25dp"
                android:layout_height="49.23dp"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="218dp"
                android:src="@drawable/treenut">
            </com.google.android.material.imageview.ShapeableImageView>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/dairy"
                android:layout_width="54.25dp"
                android:layout_height="49.23dp"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="270dp"
                android:src="@drawable/icecream">
            </com.google.android.material.imageview.ShapeableImageView>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/egg"
                android:layout_width="54.25dp"
                android:layout_height="49.23dp"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="325dp"
                android:src="@drawable/egg">
            </com.google.android.material.imageview.ShapeableImageView>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/shellfishEmoji"
                android:layout_width="54.25dp"
                android:layout_height="49.23dp"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="377dp"
                android:src="@drawable/shrimp">
            </com.google.android.material.imageview.ShapeableImageView>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/wheat"
                android:layout_width="54.25dp"
                android:layout_height="49.23dp"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="430dp"
                android:src="@drawable/wheat">
            </com.google.android.material.imageview.ShapeableImageView>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/soyEmoji"
                android:layout_width="52dp"
                android:layout_height="48dp"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="490dp"
                android:src="@drawable/shoyu">
            </com.google.android.material.imageview.ShapeableImageView>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/fishEmoji"
                android:layout_width="52dp"
                android:layout_height="48dp"
                android:layout_marginLeft="245dp"
                android:layout_marginTop="545dp"
                android:src="@drawable/fish_emoji">
            </com.google.android.material.imageview.ShapeableImageView>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/ddLogoAcc"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginLeft="270dp"
                android:layout_marginTop="68dp"
                android:src="@drawable/dd_new_logo">
            </com.google.android.material.imageview.ShapeableImageView>

            <TextView
                android:id="@+id/description"
                android:layout_width="200dp"
                android:layout_height="111dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="75dp"
                android:fontFamily="@font/avenir_roman"
                android:text="@string/description"
                android:textColor="#000000"
                android:textSize="22sp"
                android:textStyle="normal" />

            <Button
                android:id="@+id/save_allergy"
                android:layout_width="289dp"
                android:layout_height="48dp"
                android:layout_marginLeft="65dp"
                android:layout_marginTop="605dp"
                android:background="@drawable/rectangle"
                android:fontFamily="@font/avenir_roman"
                android:onClick="submitData"
                android:text="@string/Done"
                android:textAlignment="center"
                android:textColor="@color/black"
                app:backgroundTint="#5750A74E"></Button>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="match_parent"
                android:layout_height="600dp"
                android:layout_marginLeft="85dp"
                android:layout_marginTop="160dp"
                android:gravity="left"
                android:orientation="vertical">

                <CheckBox
                    android:id="@+id/peanutCheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="@string/Allergy_1"
                    android:textSize="27sp"
                    app:layout_constraintTop_toBottomOf="@+id/textView8"
                    tools:layout_editor_absoluteX="144dp" />

                <CheckBox
                    android:id="@+id/treenutsCheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="@string/Allergy_5"
                    android:textSize="27sp"
                    app:layout_constraintTop_toBottomOf="@+id/textView12"
                    tools:layout_editor_absoluteX="132dp" />

                <CheckBox
                    android:id="@+id/dairyCheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="@string/Allergy_2"
                    android:textSize="27sp"
                    app:layout_constraintTop_toBottomOf="@+id/textView6"
                    tools:layout_editor_absoluteX="173dp" />

                <CheckBox
                    android:id="@+id/eggsCheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="@string/Allergy_4"
                    android:textSize="27sp"
                    app:layout_constraintTop_toBottomOf="@+id/textView11"
                    tools:layout_editor_absoluteX="168dp" />

                <CheckBox
                    android:id="@+id/shellfishCheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="@string/Allergy_3"
                    android:textSize="27sp"
                    app:layout_constraintTop_toBottomOf="@+id/textView9"
                    tools:layout_editor_absoluteX="141dp" />

                <CheckBox
                    android:id="@+id/wheatCheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="@string/Allergy_6"
                    android:textSize="27sp"
                    app:layout_constraintTop_toBottomOf="@+id/textView2"
                    tools:layout_editor_absoluteX="157dp" />

                <CheckBox
                    android:id="@+id/soyCheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="@string/Allergy_7"
                    android:textSize="27sp"
                    app:layout_constraintTop_toBottomOf="@+id/textView5"
                    tools:layout_editor_absoluteX="177dp" />

                <CheckBox
                    android:id="@+id/fishCheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="@string/Allergy_8"
                    android:textSize="27sp"
                    app:layout_constraintTop_toBottomOf="@+id/textView3"
                    tools:layout_editor_absoluteX="174dp" />
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>